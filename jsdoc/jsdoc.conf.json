{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"],
    "recurseDepth": 10,
    "source": {
        "include": [
            "../"
        ],
        "exclude": [
            "../node_module",
            "./"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "template-default-modificado",
        "encoding": "utf8", 
        "destination": "./docs/", 
        "recurse": true,
        "readme": "../README.md"
    }
}